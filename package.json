{
  "name": "@delgadotrueba/template-css",
  "version": "0.0.1",
  "description": "Description",
  "author": "delgadotrueba",
  "keywords": [
    "http",
    "httpMock",
    "httpMockService",
    "httpService"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/delgadotrueba/template-css"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:mjs",
    "lint": "npx eslint  -c .eslintrc.json src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky",
    "prepublishOnly": "npm run test && npm run build"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "commitlint": "19.3.0",
    "husky": "9.1.4",
    "lint-staged": "15.2.8",
    "prettier": "3.3.3"
  },
  "dependencies": {
    "tslib": "2.6.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/mjs/src/index.js",
  "types": "./dist/mjs/src/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "default": "./dist/cjs/src/index.js",
        "types": "./dist/cjs/src/index.d.ts"
      },
      "import": {
        "default": "./dist/mjs/src/index.js",
        "types": "./dist/mjs/src/index.d.ts"
      }
    },
    "./public-api": {
      "require": {
        "default": "./dist/cjs/src/public-api.js",
        "types": "./dist/cjs/src/public-api.d.ts"
      },
      "import": {
        "default": "./dist/mjs/src/public-api.js",
        "types": "./dist/mjs/src/public-api.d.ts"
      }
    }
  },
  "sideEffects": false
}